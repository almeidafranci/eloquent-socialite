{
    "name": "almeidafranci/eloquent-socialite",
    "description": "Stupid simple OAuth authentication with Laravel Socialite and Eloquent. Based on https://github.com/adamwathan/eloquent-oauth",
    "authors": [
    {
        "name": "Francinaldo Almeida",
        "email": "almeidafranci@users.noreply.github.com"
    }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/auth": "7.*|^8.0",
        "illuminate/session": "7.*|^8.0",
        "illuminate/database": "7.*|^8.0",
        "illuminate/http": "7.*|^8.0",
        "illuminate/routing": "7.*|^8.0",
        "illuminate/support": "7.*|^8.0",
        "laravel/socialite": "^4.4|^5.0"
    },
    "autoload": {
        "psr-4": {
            "AlmeidaFranci\\EloquentSocialite\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlmeidaFranci\\EloquentSocialite\\EloquentSocialiteServiceProvider"
            ]
        }
    }
}
